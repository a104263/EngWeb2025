extends layout

block content
  .w3-card-4
    header.w3-container.w3-green
      h1= a._id
    .w3-container 
      h2 Id: #{a._id}
      h2 Nome: #{a.nome}
      h2 Git: #{a.git}
      ul.w3-ul TPC:
        each num in Array.from({length: 9}, (x, i) => i+1)
          if a['tpc' + num]
            li TPC #{num}
    footer.w3-container.w3-green 
      address Generated by ENGWEB2025